#!groovy
// simple job for test webhooks
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    triggers { GitHub hook trigger for GITScm polling }
    options {
        buildDiscarder(logRotator(numToKeepStr: '5', artifactNumToKeepStr: '5'))
        timestamps()
    }
    stages {
        stage("_TEST_") {
            steps {
                echo " ============== TeST WeBHooKs =================="             
            }
        }
